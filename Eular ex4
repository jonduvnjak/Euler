n = 100
largest = 1

while n < 1000:
  for i in range (100, 1001):
    if i == 1000:
      i = 100
      n += 1
    else:
      result = i * n
      reverse_num = str(result)[::-1]
      #print (int(reverse_num))
      #print (result)
      if result == int(reverse_num) and result > largest:
      	largest = result
      	print (largest)
      	#print (reverse_num)
      	#print (result)
      	


